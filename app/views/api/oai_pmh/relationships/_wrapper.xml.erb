<record>
  <header>
    <identifier><%= record.uuid %></identifier>
    <datestamp><%= timestamp record.updated_at %></datestamp>
  </header>
  <metadata>
    <% case params[:metadataPrefix] %>
      <% when "oai_dc" %>
        <%= render partial: "record.oai_dc", locals: {record: record} %>
      <% when "kor" %>
        <%= render partial: "record.kor", locals: {record: record} %>
    <% end %>
  </metadata>
</record>